<script type="text/javascript">
	
	document.addEventListener("DOMContentLoaded", function() {

		/* -- Post Content - Fig Caption Generator -- */
		const postContent = document.querySelector('.post-content');
		const images = postContent.querySelectorAll('img');

		images.forEach(image => {
		  const altText = image.getAttribute('alt');

		  if (altText) {
		    const caption = document.createElement('figcaption');
		    caption.textContent = altText;
		    image.parentNode.insertBefore(caption, image.nextSibling);
		  }
		});

	});

</script>

<script type="text/javascript">
	
	document.addEventListener("DOMContentLoaded", function() {

		/* -- Post Filter - Update Label -- */
		const postFilterContainer = document.getElementById('postFilter');
		const selectors = postFilterContainer.querySelectorAll('.selector');

		selectors.forEach((selector) => {

		  selector.addEventListener('change', () => {
		    const selectedOption = selector.options[selector.selectedIndex];
		    const label = selector.nextElementSibling; // Assuming the label is immediately after the select

		    if (label.classList.contains('selector-label')) {
		      label.textContent = selectedOption.text;
		    }
		  });

		});

	});
	
</script>


<script type="text/javascript">
	
	document.addEventListener("DOMContentLoaded", function() {

		const postList = document.querySelector('.js_postList');
		const yearFilter = document.getElementById('yearFilter');
		const categoryFilter = document.getElementById('categoryFilter');
		const projectFilter = document.getElementById('projectFilter');

		function filterPosts() {
			const selectedYear = yearFilter.value;
			const selectedCategory = categoryFilter.value;
			const selectedProject = projectFilter.value;

			postList.querySelectorAll('li').forEach((post) => {
				const postYear = post.getAttribute('data-year');
				const postCategory = post.getAttribute('data-category');
				const postProject = post.getAttribute('data-project');

				if (
					(selectedYear === '' || postYear === selectedYear) &&
					(selectedCategory === '' || postCategory === selectedCategory) &&
					(selectedProject === '' || postProject === selectedProject)
				) {
					post.style.display = 'block';
				} else {
					post.style.display = 'none';
				}
			});

			/* Disable all options */
			for (let i = 1; i < yearFilter.length; i++) {
				yearFilter[i].disabled = true;
			}
			for (let i = 1; i < categoryFilter.length; i++) {
				categoryFilter[i].disabled = true;
			}
			for (let i = 1; i < projectFilter.length; i++) {
				projectFilter[i].disabled = true;
			}

			/* Enable Only Visible Options */
			postList.querySelectorAll('li').forEach((post) => {
				const post_title = post.getAttribute('data-title')
				const post_year = post.getAttribute('data-year')
				const post_category = post.getAttribute('data-category')
				const post_project = post.getAttribute('data-project')

				if ( post.style.display != 'none' ) {
					for (let i = 0; i < yearFilter.length; i++) {
						if (yearFilter[i].value === post_year) {
							yearFilter[i].disabled = false;
						}
					}
					for (let i = 0; i < categoryFilter.length; i++) {
						if (categoryFilter[i].value === post_category) {
							categoryFilter[i].disabled = false;
						}
					}
					for (let i = 0; i < projectFilter.length; i++) {
						if (projectFilter[i].value === post_project) {
							projectFilter[i].disabled = false;
						}
					}
				}

			});

		}

		yearFilter.addEventListener('change', filterPosts);
		categoryFilter.addEventListener('change', filterPosts);
		projectFilter.addEventListener('change', filterPosts);

		// Initial filtering
		filterPosts();

	});
	
</script>

