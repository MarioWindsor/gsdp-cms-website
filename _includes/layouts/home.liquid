{% include 'parts/head.liquid' %}
{% include 'parts/header.liquid' %}







<!-- Main Section -->
<section class="main-section fill-neutral-1">
	<div class="container">
		<div class="row">
			<!-- Sidebar -->
			<div class="sidebar columns large-4 space-250-top-bottom">
				{% include 'parts/sidebar.liquid' %}
			</div>
			<!-- END: Sidebar -->
			<!-- Main -->
			<div class="main columns large-7 large-offset-1 space-250-top-bottom">
				<div class="demographic row space-200-bottom">
					<div class="columns small-12 h6 space-50-bottom">Are you looking for...</div>
					<div class="columns small-12 ">
						<div class="row">
							<div class="columns large-4 space-50-right">
								<a href="#" class="card block text-white radius-50" style="background: var(--neutral-6) url('/media/image.jpg');">
									<div class="title h6 strong inline-bottom">Architecture for<br><span class="card-line">House Owners</span></div>
									<i class="fa-icon fa-solid fa-play inline-bottom h6"></i>
								</a>
							</div>
							<div class="columns large-4 space-50-left-right">
								<a href="#" class="card block text-white radius-50" style="background: var(--neutral-6) url('/media/image.jpg');">
									<div class="title h6 strong inline-bottom">Architecture for<br><span class="card-line">Businesses</span></div>
									<i class="fa-icon fa-solid fa-play inline-bottom h6"></i>
								</a>
							</div>
							<div class="columns large-4 space-50-left">
								<a href="#" class="card block text-white radius-50" style="background: var(--neutral-6) url('/media/image.jpg');">
									<div class="title h6 strong inline-bottom">Architecture for<br><span class="card-line">Students</span></div>
									<i class="fa-icon fa-solid fa-play inline-bottom h6"></i>
								</a>
							</div>
						</div>
					</div>
				</div>
				<div class="everything row space-100-bottom">
					{% render 'parts/everything.liquid' %}
				</div>
			</div>
			<!-- END: Main -->
		</div>
	</div>
</section>
<!-- END: Main Section -->




