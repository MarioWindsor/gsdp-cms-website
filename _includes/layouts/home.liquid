{% include 'parts/head.liquid' %}
{% include 'parts/header.liquid' %}







<!-- Intro Section -->
<section id="intro-section" class="intro-section" style="background-image: url('media/image.jpg'); background-size: cover;">
	<div class="row space-200-top-bottom" style="background: rgba(0, 0, 0, 0.7);">
		<div class="container">
			<div class="columns small-12 medium-6 large-4">
				<div class="intro space-25 text-white">
					<div class="h2 font-h strong space-100-bottom">Architecture that Inspires and Endures</div>
					<div class="h6 space-100-bottom">Rooted in Human-Centric Design, our unique approach combines functionality with timeless beauty!</div>
				</div>
			</div>
			<div class="columns small-12 medium-6 large-4 large-offset-4 text-light">
				<!-- Form -->
				<div class="form space-150-left-right space-150-top-bottom radius-50" style="backdrop-filter: blur(10px) brightness(150%); -webkit-backdrop-filter: blur(10px) brightness(150%); background: rgba(143, 0, 255, 0.5);">
					<form action="" method="post">
						<div class="h5">Sign-up for a FREE<br>Online Consultation!</div>
						<label class="form-field block space-25-top" for="fullName">
							<span class="label strong inline space-min-bottom">Full Name</span>
							<input class="input-field block" type="text" name="fullName">
						</label>
						<label class="form-field block space-25-top" for="email">
							<span class="label strong inline space-min-bottom">Email ID</span>
							<input class="input-field block" type="text" name="email">
						</label>
						<label class="form-field block space-25-top" for="phone">
							<span class="label strong inline space-min-bottom">Phone Number <span class="w-400">(Request a Call-Back)</span></span>
							<input class="input-field block" type="text" name="phone">
						</label>
						<label class="form-field block space-25-top" for="phone">
							<span class="label strong inline space-min-bottom invisible">Submit</span>
							<button class="button block strong fill-light text-left" type="submit">Send</button>
						</label>
					</form>
				</div>
				<!-- END: Form -->
			</div>
		</div>
	</div>
</section>
<!-- END: Intro Section -->





<!-- Main Section -->
<section class="main-section fill-light">
	<div class="container">
		<div class="row">
			<!-- Main -->
			<div class="main columns small-12 large-7 space-200-top-bottom">
				<!-- Demographic -->
				<div class="demographic row space-200-bottom hidden">
					<div class="columns small-12 h6 space-50-bottom">Are you looking for...</div>
					<div class="columns small-12 ">
						<div class="row">
							<div class="columns large-4 space-50-right">
								<a href="#" class="card block text-white radius-50" style="background: var(--neutral-6) url('/media/image.jpg');">
									<div class="title h6 strong inline-bottom">Architecture for<br><span class="card-line">House Owners</span></div>
									<i class="fa-icon fa-solid fa-play inline-bottom h6"></i>
								</a>
							</div>
							<div class="columns large-4 space-50-left-right">
								<a href="#" class="card block text-white radius-50" style="background: var(--neutral-6) url('/media/image.jpg');">
									<div class="title h6 strong inline-bottom">Architecture for<br><span class="card-line">Businesses</span></div>
									<i class="fa-icon fa-solid fa-play inline-bottom h6"></i>
								</a>
							</div>
							<div class="columns large-4 space-50-left">
								<a href="#" class="card block text-white radius-50" style="background: var(--neutral-6) url('/media/image.jpg');">
									<div class="title h6 strong inline-bottom">Architecture for<br><span class="card-line">Students</span></div>
									<i class="fa-icon fa-solid fa-play inline-bottom h6"></i>
								</a>
							</div>
						</div>
					</div>
				</div>
				<!-- END: Demographic -->
				<!-- Posts -->
				<div class="posts row">
					<ul class="post-listing columns small-12" style="list-style-type: none;">
						{% for post in collections.post %}
						<li style="margin: 0; padding-bottom: var(--space-100);">
							<div class="post box-shadow">
								<div class="radius-50 fill-white no-overflow">
									<div class="thumbnail" style="background: url('{{ post.data.thumbnail }}'); padding-bottom: 60%; background-size: cover;">
									</div>
									<div class="content space-100-left-right space-100-top">
										<span class="small text-violet space-min-right">{{ post.data.project }}</span>
										<span class="small text-violet space-min-right">{{ post.data.client }}</span>
										<span class="small text-violet space-min-right">{{ post.data.site }}</span>
										<a class="title block text-violet h6 strong" href="{{ post.url }}">{{ post.data.title }}</a>
										<div class="description p space-25-top-bottom">
											<p>{{ post.data.description }}</p>
										</div>
									</div>
									<div class="meta row space-50-bottom">
										<div class="columns small-9 inline-middle space-100-left no-wrap no-overflow">
											<span class="small text-neutral-3 space-min-right">{{ post.data.year }}</span>
											<span class="small text-neutral-3 space-min-right">{{ post.data.category }}</span>
											<span class="small text-neutral-3 space-min-right">{{ post.data.status }}</span>
										</div>
										<div class="columns small-3 inline-middle text-right">
											<a class="button text-pink p strong" href="{{ post.url }}"><span class="hide-small">Explore </span><span class="material-symbols-rounded inline-middle" style="font-size: var(--h3); line-height: 0; font-weight: 700;">arrow_right</span></a>
										</div>
									</div>
								</div>
							</div>
						</li>
						{% endfor %}
					</ul>
				</div>
				<!-- End: Posts -->
			</div>
			<!-- END: Main -->
			<!-- Sidebar -->
			<div class="sidebar columns small-12 large-4 large-offset-1 space-200-top-bottom">
				{% include 'parts/sidebar.liquid' %}
			</div>
			<!-- END: Sidebar -->
		</div>
	</div>
</section>
<!-- END: Main Section -->




